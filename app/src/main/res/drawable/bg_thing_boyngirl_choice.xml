<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape
            android:shape="rectangle">
        <gradient
            android:startColor="#FFEBEE"
            android:endColor="#E1F5FE" />
        <corners
            android:radius="10dp"/>
        <stroke
            android:color="#F06292"
            android:width="10dp"/>
        </shape>
    </item>
    <item>
        <shape
            android:shape="rectangle">
            <solid android:color="@android:color/transparent"/>
            <corners
                android:radius="10dp"/>
            <stroke
                android:color="#4DD0E1"
                android:width="5dp"/>
        </shape>
    </item>
</layer-list>